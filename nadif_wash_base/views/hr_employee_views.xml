<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>
        <record model="ir.ui.view" id="view_employee_form_inherits">
            <field name="name">view_employee_form_inherits</field>
            <field name="model">hr.employee</field>
            <field name="inherit_id" ref="kzm_payroll_ma.view_employee_form_inherits"/>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <field name="date" position="after">
                    <field name="date_departure"/>
                    <field name="net_salary"/>
                </field>
                <field name="job_id" position="after">
                    <field name="category"/>
                </field>
                <field name="study_field" position="before">
                    <field name="level_study"/>
                </field>
            </field>
        </record>
    </data>
</odoo>